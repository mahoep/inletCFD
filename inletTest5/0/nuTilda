FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nuTilda;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            zeroGradient;
    }

    outlet
    {
        type            zeroGradient;
    }

    symmetryLine
    {
        type            symmetryPlane;
    }

    walls
    {
        type            zeroGradient;
    }

    rightWall
    {
        type            zeroGradient;
    }


    defaultFaces
    {
        type            empty;
    }
}

