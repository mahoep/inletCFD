FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.001";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];


internalField   uniform 1;

boundaryField
{
    bottomEmptyFaces
    {
        type            empty;
    }
    topEmptyFaces
    {
        type            empty;
    }
    inlet
    {
        type            turbulentIntensityKineticEnergyInlet;
        intensity       0.15;
        value           uniform 1;
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 1;
        value           uniform 1;
    }
    walls
    {
        type            kqRWallFunction;
        value           uniform 1;
    }
    rightWall
    {
        lowReCorrection 0;
        blending        stepwise;
        n               2;
        type            epsilonWallFunction;
        value           uniform 1;
    }
    symmetryLine
    {
        type            symmetryPlane;
    }
}


// ************************************************************************* //
